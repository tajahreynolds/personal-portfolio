{
  "name": "personal-portfolio",
  "module": "./src/server.tsx",
  "type": "module",
  "scripts": {
    "dev": "bun --hot run src/server.tsx",
    "clean": "rm -R node_modules && bun install",
    "build": "bun run build.ts && bun build src/server.tsx --outdir ./public",
    "start": "NODE_ENV=production bun public/server.js",
    "test": "bun test"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.2.2",
    "@vercel/speed-insights": "^1.0.11",
    "bun-types": "^1.1.12",
    "hono": "^4.4.4",
    "htmx.org": "^1.9.12",
    "typed-htmx": "^0.3.1",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}